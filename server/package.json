    "@types/glob": "^5.0.35",
    "@types/harmony-proxy": "^1.0.29",
    "@types/lodash": "^4.14.104",
    "@types/mocha": "^2.2.48",
    "@types/sinon": "^4.1.3",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "eslint-plugin-typescript": "^0.8.1",
