{
    "id": "org.sunbird.profile",
    "name": "sunbird profile",
    "author": "sunil<sunils@ilimi.in>",
    "version": "1.0",
    "server": {
        "routes": {
            "basepath": "/org-sunbird-profile",
            "prefix": "/api"
        },
        "databases": [{
                "type": "Cassandra",
                "path": "db/cassandra",
                "compatibility": "~1.0"
            },
            {
                "type": "ES",
                "path": "db/es",
                "compatibility": "~1.0"
            }
        ],
        "dependencies": {
            "nodeModules": true
        }
    }
}